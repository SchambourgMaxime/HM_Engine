# http://graphics.cs.williams.edu/data

mtllib default.mtl

v -0.5 0.5 -0.5
v -0.5 0.5 0.5
v 0.5 0.5 0.5
v 0.5 0.5 -0.5
v -0.5 -0.5 -0.5
v -0.5 -0.5 0.5
v 0.5 -0.5 0.5
v 0.5 -0.5 -0.5

vn 0 1 0
vn -1 0 0
vn 1 0 0
vn 0 0 -1
vn 0 0 1
vn 0 -1 0

g cube
usemtl default

f 8//6 7//6 6//6
f 8//6 6//6 5//6
f 8//5 4//5 3//5
f 8//5 3//5 7//5
f 6//4 2//4 1//4
f 6//4 1//4 5//4
f 5//3 1//3 4//3
f 5//3 4//3 8//3
f 7//2 3//2 2//2
f 7//2 2//2 6//2
f 3//1 4//1 1//1
f 3//1 1//1 2//1
